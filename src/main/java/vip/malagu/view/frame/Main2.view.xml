        <ClientEvent name="onRenderNode">var data = arg.node.get(&quot;data&quot;);
var icon = data.get(&quot;icon&quot;) || &quot;&quot;;
if (icon.indexOf(&quot;.png&quot;) == -1 &amp;&amp; icon.indexOf(&quot;.svg&quot;) == -1) {
